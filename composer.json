{
  "name": "metamodels/filter_checkbox",
  "description": "MetaModels checkbox filter",
  "keywords": [
    "contao",
    "metamodels",
    "filter",
    "checkbox"
  ],
  "type": "contao-bundle",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/filter_checkbox/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/filter_checkbox"
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/dc-general": "~2.1@dev",
    "metamodels/core": "~2.1@dev",
    "symfony/dependency-injection": "^3.3"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "~0.6",
    "phpcq/all-tasks": "~1.2",
    "contao/manager-plugin": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\FilterCheckboxBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MetaModels\\FilterFromToBundle\\Test\\": "tests"
    }
  },
  "extra": {
    "contao": {
      "transifex": {
        "project": "metamodels",
        "prefix": "filter_checkbox-",
        "languages_cto": "src/Resources/contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-develop": "2.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
